  "version": "1.0.5",
  "keywords": [
    "magento",
    "magento2",
    "vuestorefront",
    "klarna",
    "kco",
    "klarna checkout"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "require": {
    "php": ">=7.0.2",
    "magento/framework": ">=101.0.0",
    "klarna/module-core": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.5.0"
  },
  "replace": {
    "kodbruket/module-vsf-kco": "self.version"
  },
  "autoload": {
    "files": [
      "src/module-vsf-kco/registration.php"
    ],
    "psr-4": {
      "Kodbruket\\VsfKco\\": "src/module-vsf-kco"
    }
  },
  "scripts": {
    "test": [
      "Composer\\Config::disableProcessTimeout",
      "vendor/bin/phpunit -c src/module-vsf-kco/Test/phpunit.xml"
    ]
  },
  "minimum-stability": "alpha",
  "prefer-stable": true
}
